[
  {
    "name": "local",
    "patterns": [
      {
        "pattern": "^.+ (?:says|asks|exclaims|whispers|radios)[,. ]*'.*'$",
        "type": "regex"
      },
      {
        "pattern": "^'.*' .+ (?:says|asks|exclaims|radios)[,. ]*.$",
        "type": "regex"
      },
      {
        "pattern": "^You( .+)? (?:say|ask|exclaim|whisper|radio)[,. ]*'.*'$",
        "type": "regex"
      },
      {
        "pattern": "^'.*' you( .+)? (?:say|ask|exclaim|radio)[,. ]*.$",
        "type": "regex"
      },
      {
        "pattern": "^\\(OSAY\\) You say '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^\\(OSAY\\) .* says '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^.+ speaks in your mind '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^You speak through your mind '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^You sign, in Lorrdian '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^'.*' you sign, in Lorrdian$",
        "type": "regex"
      },
      {
        "pattern": "^\\[Hail from ",
        "type": "regex"
      },
      {
        "pattern": "^\\[Broadcasting Hail to ",
        "type": "regex"
      },
      {
        "pattern": "\\[INTERCOM:",
        "type": "regex"
      },
      {
        "pattern": "^.* '.*'.*radios ",
        "type": "regex"
      }
    ]
  },
  {
    "name": "commnet",
    "patterns": [
      {
        "pattern": "^CommNet ([0-9]+) \\[.*\\][()a-zA-Z<> ]*: (.*)",
        "type": "regex"
      }
    ]
  },
  {
    "name": "commnet-translated",
    "patterns": [
      {
        "pattern": "^.* buzzes '\\(Translating channel ([0-9]+)\\) (.*)'$",
        "type": "regex"
      }
    ]
  },
  {
    "name": "clan",
    "patterns": [
      {
        "pattern": "{.*}<.*>\\[[a-zA-Z'0-9- ]+\\][()<>A-Za-z ]*: ",
        "type": "regex"
      },
      {
        "pattern": "[Incoming Transmission from",
        "type": "substring"
      },
      {
        "pattern": "[Outgoing Transmission to",
        "type": "substring"
      }
    ]
  },
  {
    "name": "broadcast",
    "patterns": [
      {
        "pattern": "^Broadcasting Network \\[.*\\].*$",
        "type": "regex"
      }
    ]
  },
  {
    "name": "ooc",
    "patterns": [
      {
        "pattern": "^\\((OOC|IMM|RPC|NEWBIE)\\) [#*]?[\\w\\-()]+: .*$",
        "type": "regex"
      },
      {
        "pattern": "^\\(OOC\\) @?\\S+: .*$",
        "type": "regex"
      },
      {
        "pattern": "^\\(OOC\\) @?\\S+ \\[\\S+\\]: .*$",
        "type": "regex"
      }
    ]
  },
  {
    "name": "immchat",
    "patterns": [
      {
        "pattern": "^\\( IMM \\| CHAT \\) .* mortchats to .* '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^\\( IMM \\| CHAT \\)\\[.*\\]: '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^\\( IMM \\| CHAT \\) You mortchat to .* '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^ImmNet\\[[^\\]]+\\](?:<\\d+>)?: .*",
        "type": "regex"
      },
      {
        "pattern": "^CouncilNet\\[.*\\]: .*$",
        "type": "regex"
      },
      {
        "pattern": "^\\(R\\|P\\|C\\) .*: .*$",
        "type": "regex"
      },
      {
        "pattern": "^\\(R\\|P\\|C\\) .* '.*'$",
        "type": "regex"
      }
    ]
  },
  {
    "name": "tell",
    "patterns": [
      {
        "pattern": "^\\(OOC\\) .* tells you '.*'$",
        "type": "regex"
      },
      {
        "pattern": "^\\(OOC\\) You tell .* '.*'$",
        "type": "regex"
      }
    ]
  }
]
